<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mooc.jiangzh.springcloud.film.dao.mapper.MoocActorTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mooc.jiangzh.springcloud.film.dao.entity.MoocActorT">
        <id column="UUID" property="uuid" />
        <result column="actor_name" property="actorName" />
        <result column="actor_img" property="actorImg" />
    </resultMap>


    <select id="describeActors" resultType="com.mooc.jiangzh.springcloud.film.controller.vo.DescribeActorsRepVO">
        SELECT `uuid` AS actorId,actor_name AS actorName FROM mooc_actor_t
    </select>

</mapper>
